//Creare una classe di nome Persona con le
//variabili di istanza: cognome,nome, codice
//fiscale e città di tipo stringa.
//• La classe deve contenere un costruttore di
//default che inizializzi le variabili di istanza con NULL; un costruttore parametrico; 
//i metodi set e get ed un metodo chiamato
//AnnoNascita che a partire dal codice fiscale
//ricavi e restituisca l'anno di nascita di una
//persona.

public class Persona {
			protected String Cognome;
			protected String Nome;
			protected String CodiceFiscale;
			protected String Citta;
			//costruttore di default
			public Persona() {
				this.Cognome=null;
				this.Nome=null;
				this.CodiceFiscale=null;
				this.Citta=null;
			}
			//costruttore parametrico
			public Persona(String c, String n, String cf, String citta) {
				this.Cognome=c;
				this.Nome=n;
				this.CodiceFiscale=cf;
				this.Citta=citta;
			}
			//set e get
			public void SetCognome(String c) {
				this.Cognome=c;
			}
			public String getCognome(){
			return Cognome;
			}
			public void SetNome(String n) {
				this.Nome=n;
			}
			public String getNome(){
			return Nome;
			}
			public void SetCf(String cf) {
				this.CodiceFiscale=cf;
			}
			public String GetCf(){
			return CodiceFiscale;
			}
			
			
			


}
