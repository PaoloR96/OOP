import java.sql.*;
public class DbManager {
	//load driver
	
	public void GetConnection() {
		try {
			Class.forName("com.mysql.jdbc.Driver");
			System.out.print("Driver Mysql Caricato!");
			System.out.println();
		} catch (ClassNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	

}
