public class Studente {
	private char Matricola;
	private char Nome;
	private char Cognome;
	private int eta;
	//definisco il costruttore parametrico
	public Studente(char matr, char nome2, char cognome2,int eta) {
		this.Matricola=matr;
		this.Nome=nome2;
		this.Cognome=cognome2;
		this.eta=eta;
	}
/////////////---GET & SET----//////////////////////////
	public char GetMatricola() {
		return this.Matricola;
	}
	
	public char GetNome() {
		return this.Nome;
	}
	
	public char GetCognome() {
		return this.Cognome;
	}
	
	public int GetEt√†() {
		return this.eta;
	}
	
	public void SetMatricola(char matr) {
		this.Matricola=matr;
	}
	
	public void SetNome(char nome) {
		this.Nome=nome;
	}
	public void SetCognome(char cognome) {
		this.Cognome=cognome;
	}
	public void SetEta(int eta) {
		this.eta=eta;
	}
	
}
